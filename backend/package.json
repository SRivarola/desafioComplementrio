{
  "name": "servidor-con-express",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js --mode prod -p 9000",
    "dev": "nodemon ./src/server.js",
    "fs": "node ./src/server.js -p 8000 --persistence FS",
    "data": "node ./src/dao/data/data.mocks.js --mode dev",
    "test": "mocha --timeout 5000",
    "super": "mocha test/supertest",
    "test-simple": "artillery quick --count 40 --num 50 'http://localhost:7000/api/simple' -o ./test/performance/simple.json",
    "test-complex": "artillery quick --count 40 --num 50 'http://localhost:7000/api/complex' -o ./test/performance/complex.json",
    "test-performance": "artillery run ./test/performance/performance.test.yaml  --output ./test/performance/performance.json",
    "test-report": "artillery report ./test/performance/performance.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "artillery": "^2.0.3",
    "artillery-plugin-metrics-by-endpoint": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "commander": "^11.0.0",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.2",
    "mongoose-paginate-v2": "^1.7.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.6",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.1",
    "stripe": "^14.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3"
  }
}
